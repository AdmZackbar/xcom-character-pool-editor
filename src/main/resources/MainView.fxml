<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>

<fx:root type="BorderPane" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.151"
         xmlns:fx="http://javafx.com/fxml/1">
  <top>
    <MenuBar BorderPane.alignment="CENTER">
      <Menu mnemonicParsing="false" text="File">
        <MenuItem fx:id="itemLoad" mnemonicParsing="false" text="Load..."/>
        <MenuItem fx:id="itemQuit" mnemonicParsing="false" text="Quit"/>
      </Menu>
    </MenuBar>
  </top>
  <center>
    <Label fx:id="labelPlaceholder" text="No Data" BorderPane.alignment="CENTER"/>
  </center>
</fx:root>
